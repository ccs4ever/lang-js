{
  "name": "@rudolph9/cue",
  "version": "0.0.1",
  "publishConfig": { "registry": "https://npm.pkg.github.com/" },
  "description": "An npm package for using cuelang from js",
  "license": "Apache-2.0",
  "main": "index.js",
  "files": [
    "LICENSE",
    "index.js",
    "src/*",
